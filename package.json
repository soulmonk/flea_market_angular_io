    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jasmine": "^3.5.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "rxjs": "^6.5.4",
