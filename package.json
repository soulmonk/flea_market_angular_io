{
  "name": "ndfsm_js",
  "version": "1.2.1",
  "description": "NDFSM frontend on Angular",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "serve:prod_proxy": "ng serve --proxy-config proxy.conf.json",
    "serve:prod": "ng serve --proxy-config proxy-local.conf.json --configuration production",
    "serve": "ng serve --port 3500",
    "serve:hmr:prod": "ng serve --proxy-config proxy.conf.json --hmr -e=hmr",
    "serve:hmr": "ng serve --proxy-config proxy-local.conf.json --hmr -e=hmr",
    "build": "ng build",
    "build:prod": "ng build --configuration production --build-optimizer --vendor-chunk",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "keywords": [
    "angular.io"
  ],
  "author": "Vitalii Melnykov (fsoulmonk@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "^13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/router": "^13.2.2",
    "@angular/service-worker": "^13.2.2",
    "@apollo/client": "^3.5.8",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "apollo-angular": "^3.0.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.21.0",
    "date-fns": "^2.28.0",
    "font-awesome": "^4.7.0",
    "graphql": "^16.3.0",
    "jasmine": "^4.0.2",
    "normalize.css": "^8.0.1",
    "rxjs": "^7.5.4",
