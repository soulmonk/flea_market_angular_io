{
  "name": "ndfsm_js",
  "version": "1.0.0",
  "description": "NDFSM frontend on Angular2",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "serve": "ng serve --proxy-config proxy-local.conf.json",
    "serve:hmr:prod": "ng serve --proxy-config proxy.conf.json --hmr -e=hmr",
    "serve:hmr": "ng serve --proxy-config proxy-local.conf.json --hmr -e=hmr",
    "build": "ng build",
    "build:split": "rm -rf dist/templates && mkdir dist/templates && mv dist/assets/index.html dist/templates",
    "copy-to-server": "rm -rf ../ndfsm_server/dist/* && cp -r ./dist/* ../ndfsm_server/dist",
    "build:prod": "ng build --target production --build-optimizer --vendor-chunk",
    "pack": "npm run build && npm run build:split",
    "pack:prod": "npm run build:prod && npm run build:split",
    "update": "npm i && npm run pack:prod && npm run copy-to-server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "keywords": [
    "angular.io"
  ],
  "author": "Vitalii Melnykov (fsoulmonk@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/cdk": "^5.0.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/material": "^5.0.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jasmine": "^3.1.0",
    "jquery": "^1.9.1",
    "ngrx-store-freeze": "^0.2.1",
    "rxjs": "^5.5.6",
    "socket.io-client": "^2.0.4",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/language-service": "^5.1.3",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.4.7",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "pre-commit": "^1.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^2.6.2"
  },
  "pre-commit": [
    "lint"
  ]
}
